<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.supermap.android.themesamples"
    android:versionCode="1"
    android:versionName="1.0" >

    <!-- 在SD卡中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- 向SD卡写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="3"
        android:targetSdkVersion="8" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".SampleLauncher"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SampleList" />
        <activity
            android:name=".ThemeDotDensityDemo"
            android:configChanges="orientation"
            android:label="@string/app_name" />
        <activity
            android:name=".ThemeGraduatedSymbolDemo"
            android:configChanges="orientation"
            android:label="@string/app_name" />
        <activity
            android:name=".ThemeGraphDemo"
            android:configChanges="orientation"
            android:label="@string/app_name" />
        <activity
            android:name=".ThemeGridRangeDemo"
            android:configChanges="orientation"
            android:label="@string/app_name" />
        <activity
            android:name=".ThemeGridUniqueDemo"
            android:configChanges="orientation"
            android:label="@string/app_name" />
        <activity
            android:name=".ThemeLabelDemo"
            android:configChanges="orientation"
            android:label="@string/app_name" />
        <activity
            android:name=".ThemeRangeDemo"
            android:configChanges="orientation"
            android:label="@string/app_name" />
        <activity
            android:name=".ThemeUniqueDemo"
            android:configChanges="orientation"
            android:label="@string/app_name" />
    </application>

</manifest>